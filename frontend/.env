# Build Configuration
CI=false
GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true

# Disable ESLint warnings as errors in production build
ESLINT_NO_DEV_ERRORS=true
DISABLE_ESLINT_PLUGIN=false
TSC_COMPILE_ON_ERROR=true

# API Base URLs for production (change these to your actual deployed backend URLs)
REACT_APP_AUTH_API_URL=http://localhost:3000/api
REACT_APP_SYMPTOM_API_URL=http://localhost:5000
REACT_APP_PRESCRIPTION_API_URL=http://localhost:8000/api

# Public URL (will be set by Netlify automatically)
# PUBLIC_URL=

# Build optimization
INLINE_RUNTIME_CHUNK=false